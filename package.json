  "type": "module",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "SET NODE_OPTIONS=--openssl-legacy-provider && vuepress dev docs --clean-cache",
    "docs:dev": "SET NODE_OPTIONS=--openssl-legacy-provider && vuepress dev docs",
    "docs:update-package": "SET NODE_OPTIONS=--openssl-legacy-provider && pnpm dlx vp-update"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "^2.0.0-rc.7",
    "@vuepress/theme-default": "^2.0.0-rc.11",
    "vue": "^3.4.0",
    "vuepress": "^2.0.0-rc.7"
  }
}
